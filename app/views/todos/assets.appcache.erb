CACHE MANIFEST
# Version <%= appcache_version %>

# Javascripts
<%- %w{jquery.min jquery-ui.min underscore-min backbone-min backbone-patch todos}.each do |f| %>
  <%= javascript_path(f) %>
<%- end %>

# Stylesheets
<%- %w{todos}.each do |f| %>
  <%= stylesheet_path(f) %>
<%- end %>

<%- if mobile? %>
  <%= stylesheet_path('todos-mobile') %>
<%- end %>
